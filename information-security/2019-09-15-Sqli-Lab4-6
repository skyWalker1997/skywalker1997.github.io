        <article id="post-2019-09-15-Sqli-Lab4-6" class="article article-type-post" itemscope itemprop="blogPost">
    
        <h1 class="article-title">2019-09-01-Sqli-Lab1-3</h1>
    
    <div class="article-meta">
        
        
        
        <span class="book">
            
                <a href="javascript:" data-rel="信息安全">信息安全</a>
            
        </span>
        
        
        <span class="tag">
            
            <a href="javascript:" class="color1">Web安全</a>
            
            <a href="javascript:" class="color4">SQLI-Lab</a>
            
        </span>
        
    </div>
    <div class="article-meta">
        
        创建时间:<time class="date" title='更新时间: 2019-09-25 10:41:57'>2019-09-01 19:43</time>
        
    </div>
